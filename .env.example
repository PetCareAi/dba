# PetCare DBA Admin - Exemplo de Configuração
# Copie este arquivo para .env e preencha com os valores reais

# Configurações da Aplicação
APP_TITLE=PetCare DBA Admin
APP_VERSION=1.0.0
DEBUG_MODE=True

# Configurações de Administrador
ADMIN_USERNAME=seu_usuario_admin
ADMIN_PASSWORD=sua_senha_segura
ADMIN_EMAIL=admin@seudominio.com

# Configurações de Tema
PRIMARY_COLOR=#2E8B57
SECONDARY_COLOR=#90EE90

# Supabase Configuration
SUPABASE_URL=https://seu-projeto.supabase.co
SUPABASE_ANON_KEY=sua_chave_anonima_supabase
SUPABASE_SERVICE_KEY=sua_chave_service_role_supabase